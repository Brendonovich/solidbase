  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@solidjs/start": "^1.0.8",
    "prettier": "4.0.0-alpha.8",
    "solid-js": "^1.9.1",
    "typescript": "^5.6.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@solidjs/start": "^1.0.8",
    "solid-js": "^1.9.1"
  },
  "packageManager": "pnpm@8.14.0",
